<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SellerAddController">
    <children>
        <Pane layoutX="6.0" layoutY="8.0" prefHeight="384.0" prefWidth="692.0">
            <children>
                <TextField id="sellerName" fx:id="sellerName" layoutX="14.0" layoutY="39.0" prefHeight="35.0" prefWidth="327.0" />

                <TextField id="sellerLastname" fx:id="sellerLastname" layoutX="351.0" layoutY="39.0" prefHeight="35.0" prefWidth="327.0" />

                <Button fx:id="buttonAddSeller" layoutX="14.0" layoutY="88.0" mnemonicParsing="false" onAction="#addSeller" prefHeight="35.0" prefWidth="663.0" text="Add" />

                <TableView fx:id="sellerTable" layoutX="15.0" layoutY="137.0" prefHeight="232.0" prefWidth="663.0">
                    <columns>
                        <TableColumn fx:id="sellerIdColumn" prefWidth="71.0" text="Seller ID" />
                        <TableColumn fx:id="sellerNameColumn" minWidth="0.0" prefWidth="261.0" text="Seller Name" />
                        <TableColumn fx:id="sellerLastnameColumn" minWidth="0.0" prefWidth="329.0" text="Seller Lastname" />
                    </columns>
                </TableView>

                <Label alignment="CENTER" layoutX="15.0" layoutY="16.0" prefHeight="26.0" prefWidth="327.0" text="*Seller Name" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Label>

                <Label alignment="CENTER" layoutX="351.0" layoutY="16.0" prefHeight="26.0" prefWidth="327.0" text="*Seller Lastname" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</AnchorPane>