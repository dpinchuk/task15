<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SellerController">
    <children>
        <Pane layoutX="5.0" layoutY="9.0" prefHeight="384.0" prefWidth="692.0">
            <children>

                <TableView fx:id="sellerTable" layoutY="46.0" prefHeight="337.0" prefWidth="692.0">
                    <columns>
                        <TableColumn fx:id="sellerIdColumn" prefWidth="71.0" text="Seller ID" />
                        <TableColumn fx:id="sellerNameColumn" minWidth="0.0" prefWidth="300.0" text="Seller Name" />
                        <TableColumn fx:id="sellerLastnameColumn" minWidth="0.0" prefWidth="320.0" text="Seller Lastname" />
                    </columns>
                </TableView>
                
                <Button fx:id="buttonSellerDelete" mnemonicParsing="false" onAction="#removeSeller" prefHeight="40.0" prefWidth="692.0" text="Delete">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Button>

            </children>
        </Pane>
    </children>
</AnchorPane>
