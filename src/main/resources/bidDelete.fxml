<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BidController">
    <children>
        <Pane layoutX="5.0" layoutY="9.0" prefHeight="384.0" prefWidth="692.0">
            <children>

                <TableView fx:id="bidTable" layoutY="46.0" prefHeight="337.0" prefWidth="692.0">
                    <columns>
                        <TableColumn fx:id="bidIdColumn" prefWidth="71.0" text="Bid ID" />
                        <TableColumn fx:id="bidStepColumn" minWidth="0.0" prefWidth="208.0" text="Bid Step" />
                        <TableColumn fx:id="bidCurrentColumn" minWidth="0.0" prefWidth="209.0" text="Bid Current" />
                        <TableColumn fx:id="buyerIdColumn" minWidth="0.0" prefWidth="106.0" text="Buyer ID" />
                        <TableColumn fx:id="productIdColumn" minWidth="0.0" prefWidth="97.0" text="Product ID" />
                    </columns>
                </TableView>

                <Button fx:id="buttonBidDelete" mnemonicParsing="false" onAction="#removeBid" prefHeight="40.0" prefWidth="692.0" text="Delete">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Button>

            </children>
        </Pane>
    </children>
</AnchorPane>
